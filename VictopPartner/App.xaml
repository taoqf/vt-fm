             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="themes\Common.xaml"/>
                <ResourceDictionary Source="themes\Plugin.xaml"/>
                <ResourceDictionary Source="themes\PortalWindow.xaml"/>
                <ResourceDictionary Source="themes\ThemesWindow.xaml"/>
