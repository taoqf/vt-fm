    <ControlTemplate x:Key="ExpanderToggleButton"
		TargetType="{x:Type ToggleButton}">
        <Border x:Name="Border"
                Padding="{TemplateBinding Padding}"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="0,1,0,1">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CheckStates">
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)" Storyboard.TargetName="CollapsedArrow">
                                <EasingDoubleKeyFrame KeyTime="0" Value="180"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Unchecked" />
                    <VisualState x:Name="Indeterminate" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="20" />
                </Grid.ColumnDefinitions>
                <Path x:Name="CollapsedArrow" Grid.Column="1"
					HorizontalAlignment="Center"
					VerticalAlignment="Center"
                    Fill="{TemplateBinding Foreground}"
					Data="M 0 0 L 4 4 L 8 0 Z" RenderTransformOrigin="0.5,0.5">
                    <Path.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Path.RenderTransform>
                </Path>
                <TextBlock 
					VerticalAlignment="Center"
                    Foreground="{TemplateBinding Foreground}"
					Text="{TemplateBinding Content}"/>
            </Grid>
        </Border>
    </ControlTemplate>

    <Style x:Key="ExpanderFirstLevel" TargetType="{x:Type Expander}">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Margin" Value="0,2,0,0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Expander}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="26" />
                            <RowDefinition x:Name="ContentRow" Height="0" />
                        </Grid.RowDefinitions>
                        <ToggleButton 
                                    OverridesDefaultStyle="True"
                                    Padding="6,0,0,0"
                                    Background="{DynamicResource FirstMenuItemNormalBackground}"
                                    BorderBrush="{DynamicResource FirstMenuItemNormalBorderBrush}"
                                    BorderThickness="0,1,0,1"
                                    Foreground="{DynamicResource FirstMenuItemNormalForeground}"
									Template="{DynamicResource ExpanderToggleButton}"
                                    Content="{TemplateBinding Header}"
									IsChecked="{Binding IsExpanded, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}">
                        </ToggleButton>
                        <Border x:Name="Content"
							Grid.Row="1">
                            <ContentPresenter 
                                Focusable="false" 
