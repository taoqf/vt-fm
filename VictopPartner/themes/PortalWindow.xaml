                                            </ScrollViewer>
                                        </Popup>
                                    </Grid>
                                </Grid>

                                <ControlTemplate.Triggers>
                                    <Trigger  Property="IsSelected" Value="true">
                                        <Setter Property="Background" TargetName="border" Value="{Binding CheckedIconUrl,Converter={StaticResource ImgeNameToImageUrlConverter}}"></Setter>
                                    </Trigger>
                                    <Trigger  Property="IsMouseOver" Value="true">
                                        <Setter Property="Background" TargetName="border" Value="{Binding CheckedIconUrl,Converter={StaticResource ImgeNameToImageUrlConverter}}"></Setter>
                                        <Setter Property="Foreground" Value="{DynamicResource LeftMenuOverForeground}"/>
                                    </Trigger>
                                    <Trigger SourceName="grid" Property="IsMouseOver" Value="true">
                                        <Setter Property="IsOpen" TargetName="popup" Value="true"/>
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
