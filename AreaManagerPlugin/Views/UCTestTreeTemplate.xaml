<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ToolBarTemplates="clr-namespace:Victop.Frame.Templates.ToolBarTemplates;assembly=Victop.Frame.Templates" xmlns:LeftCondtionTemplates="clr-namespace:Victop.Frame.Templates.LeftCondtionTemplates;assembly=Victop.Frame.Templates" xmlns:DataGridTemplates="clr-namespace:Victop.Frame.Templates.DataGridTemplates;assembly=Victop.Frame.Templates" x:Class="AreaManagerPlugin.Views.UCTestTreeTemplate" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="600">
    <Grid>
        <DockPanel>

            <ToolBarTemplates:UCSearchToolBarT DockPanel.Dock="Top" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>
            <LeftCondtionTemplates:UCTreeViewCondtionT DockPanel.Dock="Left" Width="120" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>
            <DataGridTemplates:UCMasterDataGridT Height="230" VerticalAlignment="Top"/>

        </DockPanel>
    </Grid>
</UserControl>
