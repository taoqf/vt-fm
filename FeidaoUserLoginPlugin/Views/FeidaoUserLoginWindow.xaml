<Window x:Class="FeidaoUserLoginPlugin.Views.FeidaoUserLoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:views="clr-namespace:FeidaoUserLoginPlugin.Views"
        xmlns:vm="clr-namespace:FeidaoUserLoginPlugin.ViewModels"
        Title="FeidaoUserLoginWindow" Height="626" Width="386" WindowStartupLocation="CenterScreen"  Style="{DynamicResource feidaologinWindowStyle}">

    <Grid x:Name="gridRoot" Height="626" Width="386" HorizontalAlignment="Center" VerticalAlignment="Center">
        <Grid.DataContext>
            <vm:FeidaoUserLoginViewModel/>
        </Grid.DataContext>
        <views:UCFeidaoUserLogin x:Name="FrontGD"></views:UCFeidaoUserLogin>
        <views:UCFeidaoSystemConfig x:Name="BackGD" Visibility="Collapsed"></views:UCFeidaoSystemConfig>
       
    </Grid>
</Window>
